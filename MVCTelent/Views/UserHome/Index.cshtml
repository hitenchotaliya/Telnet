@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}





<section class="bg-home">
    <div class="bg-overlay"></div>
    <div class="home-center">
        <div class="home-desc-center">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-9">
                        <div class="home-title text-center text-white">
                            <h5 class="small-title text-uppercase f-17 text-white-50 mb-4">Discover With TelNet</h5>
                            <h1 class="mb-4">TelNet</h1>
                        </div>
                    </div>
                </div>
                <div class="home-form-position">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="home-registration-form p-4 mb-3">
                                <form class="registration-form">
                                    <div class="row">
                                        <div class="col-lg-3 col-md-6">
                                            <div class="registration-form-box">
                                                <i class="fa"></i>
                                                @{
                                                    string email = ViewBag.CustomerEmail ?? ViewBag.UserEmail;
                                                    bool isCustomerLoggedIn = !string.IsNullOrEmpty(ViewBag.CustomerEmail);
                                                    bool isUserLoggedIn = !string.IsNullOrEmpty(ViewBag.UserEmail);
                                                }

                                                <input type="text" id="exampleInputName1" class="form-control registration-input-box" placeholder="@(isCustomerLoggedIn ? "Talent Keywords..." : (isUserLoggedIn ? "Job Keywords..." : "Talent Keywords..."))">
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-6">
                                            <div class="registration-form-box">
                                                <i class="fa fa-location-arrow"></i>
                                                <select id="select-country" class="demo-default" placeholder="Search Location...">
                                                    <option value="">Location</option>
                                                    <option value="AF">India</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-6">
                                            <div class="registration-form-box">
                                                <i class="fa fa-list-alt"></i>
                                                <select id="select-category" class="demo-default" placeholder="@(isCustomerLoggedIn ? "Talent Keywords..." : (isUserLoggedIn ? "Job Keywords..." : "Talent Keywords..."))">
                                                    <option value="">Categories...</option>
                                                    @if (isCustomerLoggedIn)
                                                    {
                                                        <option value="4">Accounting</option>
                                                        <option value="1">IT & Software</option>
                                                        <option value="3">Marketing</option>
                                                        <option value="5">Banking</option>
                                                    }
                                                    @if (isUserLoggedIn)
                                                    {
                                                        <option value="4">Kiara Techno</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-6">
                                            <div class="registration-form-box">
                                                <input type="submit" id="submit" name="send" class="submitBnt btn btn-custom btn-block " style="background-color: whitesmoke; color: black;" value="Submit">
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
