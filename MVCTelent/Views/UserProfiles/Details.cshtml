@model MVCTelent.Models.UserProfile

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/AdminLayout.cshtml";

}
<div class="card">
    <div class="card-body mt-3">
<h1>Details</h1>
        <div class="card">
            <div class="card-body mt-3">
<div>
    <h4>UserProfile</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.VideoGelleryId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VideoGellery.GelleryName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ImageGellery)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ImageGellery.GelleryName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Email)
        </dd>
    </dl>
</div>
<div>
                    <a asp-action="Edit" asp-route-id="@Model?.UserProfileId" class="btn btn-primary mt-1">Edit</a>
                    <a asp-action="Index" class="btn btn-outline-dark mt-1">Back</a>
</div>
</div>
</div>
</div>
</div>
